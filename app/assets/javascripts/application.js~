$(function() {
$( "select" )
  .change(function () {
    var str = "";
    $( "select option:selected" ).each(function() {
      str += $( this ).text() + " ";
    });
    $( '#vat' ).text( str );
  })
  .change();
 });
var primaryincome = $("#primaryincome");
var otherincome = $("#otherincome");
$(function() {
$(document).ready(function(){
    $('.rs').keyup(function(){

       var val2 = 0;
       $("input[id^='rs']").each(function()
       {
           if($(this).val().length != 0)
           {
       val2 = val2 + parseInt($(this).val()) 
           }
    });
        
        $('#Total').val(val2);
    });
});
})


